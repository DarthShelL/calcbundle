<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="darthshell\cfs\CalculateInterface" alias="cfs.calculate" />
        <service id="cfs.calculate" class="darthshell\cfs\CalculateHandler"/>
        <service id="cfs" class="darthshell\cfs\CalculateFromStringg">
            <argument key="$ci">cfs.calculate</argument>
        </service>
    </services>
</container>
