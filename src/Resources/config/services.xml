<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="cfs.handler" class="darthshell\cfs\CalculateHandler" />
        <service id="darthshell\cfs\CalculateInterface" alias="cfs.handler" />

        <service id="cfs" class="darthshell\cfs\CalculateFromString">
            <argument type="service" key="$ci" id="cfs.handler"/>
        </service>
        <service id="darthshell\cfs\CalculateFromString" alias="cfs"/>
    </services>
</container>
